<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Upload</title>
</head>
<body>
  <ul class="files">
    <% files.forEach(function(file){ %>
      <li><a href="/public/uploads/<%= file.filename %>"><%= file.originalname %></a></li>
    <% }); %>
  </ul>
  <form action="/" method="post" enctype="multipart/form-data">
    <input type="file" name="files" multiple="multiple">
    <input type="submit" value="upload">
  </form>
</body>
</html>
